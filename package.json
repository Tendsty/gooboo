    "color": "^4.2.3",
    "core-js": "^3.33.0",
    "electron-squirrel-startup": "^1.0.0",
    "js-file-download": "^0.4.12",
    "seedrandom": "^3.0.5",
    "semver": "^7.5.4",
    "v-snackbars": "^3.2.8",
    "vue": "^2.7.14",
    "vuetify": "^2.7.1",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.15",
    "@electron-forge/cli": "^6.4.0",
    "@electron-forge/maker-deb": "^6.4.0",
